    \entry{liaoStructuralBasisSizerelated2003}{article}{}{}
      \name{author}{2}{}{%
        {{un=0,uniquepart=base,hash=bcf04ea85e2546d820b924a803502f5e}{%
           family={Liao},
           familyi={L\bibinitperiod},
           given={Jun},
