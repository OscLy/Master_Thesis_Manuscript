      \strng{namehash}{20327b09edc7f90d3a4102d7b6b6e4af}
      \strng{fullhash}{66e237278530f67925a4abe9a644201e}
      \strng{fullhashraw}{66e237278530f67925a4abe9a644201e}
      \strng{bibnamehash}{66e237278530f67925a4abe9a644201e}
      \strng{authorbibnamehash}{66e237278530f67925a4abe9a644201e}
      \strng{authornamehash}{20327b09edc7f90d3a4102d7b6b6e4af}
