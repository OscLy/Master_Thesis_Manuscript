      \strng{namehash}{e4b479af834c1069df3180bed7f08e19}
      \strng{fullhash}{e4b479af834c1069df3180bed7f08e19}
      \strng{fullhashraw}{e4b479af834c1069df3180bed7f08e19}
      \strng{bibnamehash}{e4b479af834c1069df3180bed7f08e19}
      \strng{authorbibnamehash}{e4b479af834c1069df3180bed7f08e19}
