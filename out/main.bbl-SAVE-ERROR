      \field{title}{A {{Novel Left Heart Simulator}} for the {{Multi-modality Characterization}} of {{Native Mitral Valve Geometry}} and {{Fluid Mechanics}}}
      \field{volume}{41}
      \field{year}{2013}
      \field{dateera}{ce}
