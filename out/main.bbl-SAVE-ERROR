    \entry{thomasComputationalMultiscaleApproach2019}{article}{}{}
      \name{author}{3}{}{%
        {{un=0,uniquepart=base,hash=dc03495623284816177f07adddbca873}{%
           family={Thomas},
           familyi={T\bibinitperiod},
