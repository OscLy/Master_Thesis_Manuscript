      \strng{namehash}{99f662ee470c87b69e19a7cb6b45fe75}
      \strng{fullhash}{5426366149f8532657cefabd6a56275c}
      \strng{fullhashraw}{5426366149f8532657cefabd6a56275c}
      \strng{bibnamehash}{5426366149f8532657cefabd6a56275c}
      \strng{authorbibnamehash}{5426366149f8532657cefabd6a56275c}
      \strng{authornamehash}{99f662ee470c87b69e19a7cb6b45fe75}
      \strng{authorfullhash}{5426366149f8532657cefabd6a56275c}
