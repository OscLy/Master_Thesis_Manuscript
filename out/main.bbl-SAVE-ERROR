      \strng{namehash}{1b87eb5d70785165910882f17328de05}
      \strng{fullhash}{3dc9ed54f2efc9eea8b03d08f6a43581}
      \strng{fullhashraw}{3dc9ed54f2efc9eea8b03d08f6a43581}
      \strng{bibnamehash}{3dc9ed54f2efc9eea8b03d08f6a43581}
      \strng{authorbibnamehash}{3dc9ed54f2efc9eea8b03d08f6a43581}
      \strng{authornamehash}{1b87eb5d70785165910882f17328de05}
      \strng{authorfullhash}{3dc9ed54f2efc9eea8b03d08f6a43581}
