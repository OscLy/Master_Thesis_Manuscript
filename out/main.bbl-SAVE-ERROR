      \strng{namehash}{d07461d2f1424b0aa0a99727823a1bc4}
      \strng{fullhash}{4850bef3586a99f25d23aec9390e69e0}
      \strng{fullhashraw}{4850bef3586a99f25d23aec9390e69e0}
      \strng{bibnamehash}{4850bef3586a99f25d23aec9390e69e0}
      \strng{authorbibnamehash}{4850bef3586a99f25d23aec9390e69e0}
      \strng{authornamehash}{d07461d2f1424b0aa0a99727823a1bc4}
