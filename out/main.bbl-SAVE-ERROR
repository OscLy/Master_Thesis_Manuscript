      \strng{namehash}{fc5c08ef657f4bfdb2334c0c82ca29fc}
      \strng{fullhash}{fc5c08ef657f4bfdb2334c0c82ca29fc}
      \strng{fullhashraw}{fc5c08ef657f4bfdb2334c0c82ca29fc}
      \strng{bibnamehash}{fc5c08ef657f4bfdb2334c0c82ca29fc}
