      \field{issn}{2047-2404}
      \field{journaltitle}{European Heart Journal - Cardiovascular Imaging}
      \field{month}{5}
      \field{number}{5}
      \field{shortjournal}{European Heart Journal - Cardiovascular Imaging}
      \field{title}{Elongation of Chordae Tendineae as an Adaptive Process to Reduce Mitral Regurgitation in Functional Mitral Regurgitation}
      \field{volume}{17}
      \field{year}{2016}
      \field{dateera}{ce}
