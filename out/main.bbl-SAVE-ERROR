      \field{shorttitle}{The {{Living Heart Project}}}
      \field{title}{The {{Living Heart Project}}: {{A}} Robust and Integrative Simulator for Human Heart Function}
      \field{volume}{48}
      \field{year}{2014}
      \field{dateera}{ce}
      \field{pages}{38\bibrangedash 47}
