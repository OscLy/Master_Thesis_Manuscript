@article{laurenceInvestigationRegionalVariations2019,
  title = {An Investigation of Regional Variations in the Biaxial Mechanical Properties and Stress Relaxation Behaviors of Porcine Atrioventricular Heart Valve Leaflets},
  author = {Laurence, Devin and Ross, Colton and Jett, Samuel and Johns, Cortland and Echols, Allyson and Baumwart, Ryan and Towner, Rheal and Liao, Jun and Bajona, Pietro and Wu, Yi and Lee, Chung-Hao},
  date = {2019-01-23},
  journaltitle = {Journal of Biomechanics},
  shortjournal = {Journal of Biomechanics},
  volume = {83},
