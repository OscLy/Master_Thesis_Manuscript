@article{leopaldiVitroHemodynamicsValve2012,
  title = {\emph{In Vitro} Hemodynamics and Valve Imaging in Passive Beating Hearts},
  author = {Leopaldi, A. M. and Vismara, R. and Lemma, M. and Valerio, L. and Cervo, M. and Mangini, A. and Contino, M. and Redaelli, A. and Antona, C. and Fiore, G. B.},
  date = {2012-04-30},
