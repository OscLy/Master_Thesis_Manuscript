  author = {Hell, Michaela M and Emrich, Tilman and Kreidel, Felix and Kreitner, Karl-Friedrich and Schoepf, U Joseph and Münzel, Thomas and family=Bardeleben, given=Ralph Stephan, prefix=von, useprefix=true},
