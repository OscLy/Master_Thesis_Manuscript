@article{leeMechanicsTricuspidValve2019,
  title = {Mechanics of the {{Tricuspid Valve}}—{{From Clinical Diagnosis}}/{{Treatment}}, {{In-Vivo}} and {{In-Vitro Investigations}}, to {{Patient-Specific Biomechanical Modeling}}},
  author = {Lee, Chung-Hao and Laurence, Devin W. and Ross, Colton J. and Kramer, Katherine E. and Babu, Anju R. and Johnson, Emily L. and Hsu, Ming-Chen and Aggarwal, Ankush and Mir, Arshid and Burkhart, Harold M. and Towner, Rheal A. and Baumwart, Ryan and Wu, Yi},
  date = {2019-05-22},
  journaltitle = {Bioengineering},
  shortjournal = {Bioengineering (Basel)},
  volume = {6},
